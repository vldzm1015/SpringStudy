<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<bean id="memberBean" class="ex2_annotation.MemberBean">
	<property name="name" value="홍홍"></property>
	<property name="age" value="55"></property>
	<property name="message" value="지각노노"></property>
</bean>

<bean id="memberBean2" class="ex2_annotation.MemberBean">
	<property name="name" value="홍홍2"></property>
	<property name="age" value="22"></property>
	<property name="message" value="지각노노2"></property>
</bean>

<context:component-scan base-package="ex2_annotation"/>




</beans>
